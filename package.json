{
    "name": "empyry",
    "version": "0.0.0",
    "description": "The One Stop Package Registry That Is Out Of This World!",
    "author": "Dual Vipers",
    "license": "Apache-2.0",
    "homepage": "https://empyry.com",
    "bugs": {
        "url": "https://github.com/DualVipers/empyry/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DualVipers/empyry"
    },
    "private": true,
    "scripts": {
        "docs": "docsify serve docs",
        "lint": "eslint",
        "prettier": "prettier --check .",
        "audit": "yarn-audit-fix",
        "setup": "yarn install --frozen-lockfile && husky install && lerna bootstrap && lerna link"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "cz-conventional-changelog": "3.3.0",
        "docsify-cli": "4.4.4",
        "eslint": "8.13.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.4",
        "lerna": "4.0.0",
        "prettier": "2.6.2",
        "yarn-audit-fix": "9.2.2"
    },
    "resolutions": {
        "inquirer": "8.2.2",
        "minimist": "1.2.6"
    },
    "workspaces": [
        "core",
        "managers/*",
        "types"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
