{
    "name": "@empyry/core",
    "version": "0.0.1-beta.0",
    "description": "The core of Empyry",
    "private": true,
    "type": "module",
    "scripts": {
        "initiate": "yarn migrate; yarn start",
        "migrate": "cross-env MIGRATION=true node ./src/migrate.js | bunyan -o short",
        "output": "bunyan ./testing.log",
        "start": "node ./src/Empyry.js | bunyan -o short"
    },
    "dependencies": {
        "argon2": "0.29.1",
        "bunyan": "1.8.15",
        "cross-env": "7.0.3",
        "env-paths": "3.0.0",
        "express": "4.18.1",
        "fs-extra": "10.1.0",
        "hasha": "5.2.2",
        "knex": "2.3.0",
        "live-plugin-manager": "0.17.1",
        "objection": "3.0.1",
        "openapi-backend": "5.5.0",
        "semver": "7.3.7",
        "sqlite3": "5.1.1",
        "swagger-ui-dist": "4.14.2",
        "waymaker": "0.1.0"
    },
    "devDependencies": {
        "@empyry/types": "^0.0.1-beta.0",
        "yarn-audit-fix": "9.3.6"
    },
    "workspaces": {
        "nohoist": [
            "**"
        ]
    }
}
